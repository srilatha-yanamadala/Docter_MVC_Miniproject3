@model IEnumerable<Doctor_MVC_Miniproject3.Models.Doctor>
@using Doctor_MVC_Miniproject3.Models

@{
    ViewData["Title"] = "Index";
}
<body class="Doctorbody">
<h1>Index</h1>
<h3>
<a  asp-controller="Appointments">see here your appointments</a></h3>
  <!-- <a Response.Redirect(Url.Action("action", "Patientcontroller"))>see</a> -->
  <br />
  <hr />

           
<p>
    <a asp-action="Create">Add new Doctors here</a>
</p>
<br /><hr />
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.DOctorName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Specliazation)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ImageUrl)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.DOctorName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Specliazation)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ImageUrl)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.DoctorId">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.DoctorId">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.DoctorId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
</body>
